.container_page {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: white;
}

.weui-navbar {
  width: 100%;
  height: 78rpx;
  background-color:white;
  position: absolute;
  z-index: 998;
  margin-bottom: 12rpx;
}
 
 
.weui-navbar__title {
  font-weight: 500;
  font-size: 28rpx;
  margin-top: -10rpx;
  color: #626466;
}

.weui-bar__item_on .weui-navbar__title {
  /* color:#F6B352; */
  font-weight: 700;
  font-size: 28rpx;
  margin-top: -10rpx;
}

.b-line{
  height: 4rpx;
  width: 48rpx;
  background-color: #F6B352;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%,0);

}



.weui-navbar__slider {
  background-color: #0091b0;
}
.weui-tab__panel{
  width: 100%;
  height: 100%;
}
/*聊天记录  */
.weui-tab__content {
  width: 100%;
  height: 100%;
}

.order-img {
  width: 112rpx;
  height: 112rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
  margin-top: 15rpx;
}

.order-time {
  display: inline-block;
  font-size: 24rpx;
  color: rgba(147, 150, 153, 1);
}

.one-order {
  height: 192rpx;
  width: 670rpx;
  margin: 0 auto;
  border-bottom: 2rpx solid rgba(240, 242, 245, 1);
  display: flex;
  flex-flow: row;
  align-items: center;

}
.order-title {
  font-weight: 700;
  width: 400rpx;
  font-size: 32rpx;

}
.order-price{
  font-size: 36rpx;
  font-weight: 500;

}
.basic {
  height: 115rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.zeng {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  left: 120rpx;
}