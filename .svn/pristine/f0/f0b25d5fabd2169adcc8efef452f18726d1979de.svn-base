.container_page {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: white;
  justify-content: center;
  align-items: center;
}
.page-content {
  width: 100%;
  height: 256px;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.page-content .account-label{
  width: 100%;
  min-height: 32rpx;
  color: #313233;
  font-size: 28rpx;
  font-weight:400;
  display: flex;
  font-weight: 500;
  justify-content: center;
  align-items: center;  
}
.page-content .account-balance {
  width: 100%;
  min-height: 80rpx;
  color: #313233;
  font-weight: 500;
  font-size: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.page-banlance-btn{
  width: 80%;
  height: 96rpx;
  margin-top: 32rpx;
  border-radius: 64rpx;
  color: white;
  background-color: #F6B352;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 500;
  font-size: 36rpx;
}