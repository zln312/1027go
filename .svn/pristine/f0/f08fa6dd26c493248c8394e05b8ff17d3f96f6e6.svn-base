#video-livePlayer {
  width: 100%;
  height: 100%;
}
.live-player-logo {
  width: 100%;
  height: 100%;
  z-index: 1;
  position: absolute;
}




.logo_v {
  width: 100%;
  height: 72rpx;
  position: fixed;
  top: 0rpx;
  z-index: 3;
  display: flex;
  align-items: center;
  padding-top: 8rpx;
  padding-bottom: 8rpx;
  padding-left: 20rpx;
  background-color: rgba(98, 98, 98, 0.4);
}
 .logo-img {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  margin-left: 16rpx;
}
.logo_content {
  flex: 1 1;
  margin-left: 12rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  align-self: stretch
}
#live_name {
  flex: 0 0 100%;
  color: aliceblue;
  font-style: bold;
  font-size: 32rpx;
}
#live_inf{
  flex: 0 0 100%;
    font-size: 20rpx;
    color: aliceblue;
    font-style: normal;
}





.doommview {
height: 100%;
width: 100%;
position: absolute;
top: 0;
}



.page-header-play-content .left image.impeach {
  width: 52rpx;
  height: 52rpx;
  margin-left: 2vw;
}
.page-header-play-content {
  width: 100%;
  height: 72rpx;
  z-index: 3;
  position: absolute;
  bottom: 0rpx;
  display: flex;
  padding-top: 8rpx;
  padding-bottom: 8rpx;
  background-color: rgba(98, 98, 98, 0.4);
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.page-header-play-content .left {
  width: 50%;
  height: 100%;
  margin-left: 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.page-header-play-content .left image {
  width: 46rpx;
  height: 38rpx;
}

.page-header-play-content .right {
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.page-header-play-content .right image {
  width: 72rpx;
  height: 72rpx;
  margin-right: 30rpx;
}






