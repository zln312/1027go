.container_page {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: white;
  justify-content: center;
  align-items: center;
}

.divider-line {
  width: 100%;
  min-height: 2rpx;
  background-color: rgb(244, 244, 244);
}
.page-header{
  width: 90%;
  height: 288rpx;
  display: flex;
  margin-top: 32rpx;
  background-color: white;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  box-shadow:0px 16rpx 32rpx 0px rgba(0,0,0,0.06);
  border-radius:16rpx;
}
.page-header .title-content{
  width: 100%;
  height: 72rpx;
  display: flex;
  margin-bottom: 12rpx;
  flex-direction: row;
  justify-content:center;
  align-items: center;
}
.page-header .title-content .left{
  width: 80%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content:flex-start;
  align-items: center;
}
.page-header .title-content .right{
  width: 20%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content:flex-end;
  align-items: center;
}
 

.page-header .title-content .right .status-tx{
  width: 70%;
  height: 100%;
  color: #939699;
  font-size: 30rpx;
  display: flex;
  justify-content:center;
  align-items: center;  
}
 .status-wa{
  margin-right: 30rpx;
  width: 70%;
  height: 100%;
  color: #F6B352;
  font-size: 30rpx;
  display: flex;
  justify-content:center;
  align-items: center;  
}
.page-header .title-content .right .status-icon{
  width: 30%;
  height: 100%;
  display: flex;
  justify-content:center;
  align-items: center;  
}
.page-header .title-content .right .status-icon image {
    width: 32rpx;
    height: 32rpx;
}
.page-header .title-content .left .icon{
  width: 15%;
  height: 100%;
  display: flex;
  justify-content:center;
  align-items: center;
}
.page-header .title-content .left .icon image{
  width: 48rpx;
  height: 48rpx;
}
.page-header .title-content .left .name{
  height: 100%;
  display: flex;
  justify-content:flex-start;
  align-items: center;
  color: #313233;
  font-weight: 500;
  font-size: 32rpx;
}
.page-header .title-content .left .status{
  height: 42rpx;
  width: 114rpx;
  display: flex;
  font-size: 20rpx;
  color: white;
  margin-left: 12rpx;
  border-radius: 20rpx;
  background-color: #939699;
  justify-content:center;
  align-items: center;
}
 
.status_on {
  display: flex;
  justify-content:center;
  align-items: center;
  margin-left: 12rpx;
  height: 42rpx;
  width: 114rpx;
}
.data-content{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content:flex-start;
  align-items: flex-end;  
  margin-right: 10rpx;
}
.data-content .make-collections {
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content:center;
  align-items: flex-start;
}
.make-collections .label{
  width: 50%;
  height: 100%;
  display: flex;
  justify-content:flex-start;
  align-items: flex-end;
  color: #939699;
  font-weight: 400;
  font-size: 26rpx;
}
.make-collections .number{
  width: 50%;
  height: 100%;
  display: flex;
  justify-content:center;
  align-items: flex-start;
  color: #313233;
  font-weight: 500;
  font-size: 42rpx; 
}
.data-content .balance-content {
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content:center;
  align-items: center;  
}
.balance-content .label{
  width: 50%;
  display: flex;
  justify-content:flex-start;
  align-items: flex-end; 
  color: #939699;
  font-weight: 400;
  font-size: 26rpx; 
}
.balance-content .number{
  width: 50%;
  display: flex;
  justify-content:flex-start;
  align-items: flex-start; 
  color: #313233;
  font-weight: 500;
  font-size: 42rpx; 
}
.canvas {
  width: 100%;
  height: 320px;
}
.page-content {
  width: 100%;
  display: flex;
  margin-top: 32rpx;
  flex-direction: column;
  justify-content:flex-start;
  align-items: center;  
}
.page-content .title{
  width: 90%;
  min-height: 36rpx;
  margin-left: 36rpx;
  margin-top: 0rpx;
  font-weight: 600;
  font-size: 32rpx;
  color: #313233;
  line-height: 30rpx;
}

.mycanvas {
  width: 100vw;
  height: 40vh;
}
.title-bottom {
  width: 90%;
  font-size: 32rpx;
  font-weight: 600;
  color: #313233;
  margin-top: 20rpx;

}

.line-bottom {
  height: 10rpx;
  width: 100%;
  background-color: #F2F4F5;
}
.mytools {
  width: 90%;
  display: flex;
  margin-top: 20rpx;
}
.outer-tool {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-right: 65rpx;
  font-size: 28rpx;
}

.mytools image {
  width: 96rpx;
  height: 96rpx;
}
.withdraw-btn {
  display: flex;
  align-items: center;
  color: #F6B352;
  font-size: 26rpx;
  overflow: hidden;
  width: 50%;

}

.my-arrows {
  display: inline-block;
  width: 17rpx;
  height: 17rpx;
  border-top: 1rpx solid #F6B352;
  border-right: 1rpx solid #F6B352;
  transform: rotate(45deg);
}