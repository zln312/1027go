Page({

  /**
   * 页面的初始数据
   */
  data: {
    url: '',
  },

  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function(options) {
      let _that = this;
      let _data = _that.data;
      console.log(options.linkurl);
      _that.setData({
        url: options.linkurl
      })
  },

  /**
   * 生命周期函数--监听页面初次渲染完成
