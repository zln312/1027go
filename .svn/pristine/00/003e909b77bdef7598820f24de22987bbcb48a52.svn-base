.container_page {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: #F5F7FA;
  justify-content: center;
  align-items: center;
}
.divider-line {
  width: 100%;
  min-height: 2rpx;
  background-color: rgb(244, 244, 244);
}
.scroll-content {
  width: 100%;
  height: 100%;
  white-space:nowrap;
}
.page-cert-baseInfo{
  width: 100%;
  margin-top: 32rpx;
  padding-left: 32rpx;
  padding-right: 32rpx;
  background-color: white;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.page-credentials-images{
  width: 100%;
  margin-bottom: 16rpx;
  padding-bottom: 16rpx;
  display: flex;
  margin-top: 32rpx;
  background-color: white;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center; 
}
.divider-line {
  width: 100%;
  min-height: 2rpx;
  background-color: rgb(244, 244, 244);
}
.divider-line1 {
  width: 90%;
  min-height: 2rpx;
  background-color: rgb(244, 244, 244);
}
.contacts-content{
  width: 90%;
  height: 96rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.contacts-content .label{
  width: 25%;
  height: 96rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  color: #626466;
  font-size: 28rpx;
  font-weight: 400;
}
.contacts-content .ipt{
  width: 75%;
  height: 96rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  color: #626466;
  font-size: 28rpx;
  font-weight: 400;
}
.contacts-content .ipt input{
  width: 100%;
  height: 100%;
}
.page-credentials-images .title{
  width: 90%;
  min-height: 32rpx;
  margin-top: 16rpx;
  margin-bottom: 16rpx;
  color: #626466;
  font-weight: 400;
  font-size: 28rpx;
  justify-content: flex-start;
  align-items: center;
}
.page-credentials-images .img{
  width: 90%;
  min-height: 320rpx;
  margin-top: 12rpx;
  display: flex;
  flex-direction: column;
  background-color: #FBF6DB;
  justify-content: center;
  align-items: center;  
}

.show-img {
   height: 320rpx;
   width: 90vw;
}
.show-outer{
 font-size: 20rpx;
  width: 90%;
  display: flex;
  flex-direction: column;
   justify-content: center;
  align-items: center;  
}
.page-credentials-images .img image{
    width: 96rpx;
    height: 96rpx;
}
.page-credentials-images .img .img_title{
  min-height: 32rpx;
  font-size: 28rpx;
  margin-top: 16rpx;
  font-weight: 400;
  color: #F6B352;
}

.page-banlance-btn{
  width: 80%;
  height: 96rpx;
  margin-top: 32rpx;
  border-radius: 64rpx;
  color: white;
  background-color: #F6B352;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 500;
  font-size: 36rpx;
}

.page-banlance-btn-no{
  width: 80%;
  height: 96rpx;
  margin-top: 32rpx;
  border-radius: 64rpx;
  color: white;
  background-color: rgb(192, 192, 192);
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 500;
  font-size: 36rpx;
}
