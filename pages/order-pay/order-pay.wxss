.container_page {
  width: 100%;
  height: 100vh;
  flex-direction: column;
  overflow: hidden;
  position: absolute;
  background-color: #EFF1F4;
}

.page-header {
  width: 100%;
  background-color: white;
  margin-top: 16rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.page-content {
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color: white;
  margin-top: 16rpx;
  justify-content: flex-start;
  align-items: center;
}

.page-header-wx {
  width: 90%;
  height: 50%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  padding: 24rpx 0;
}

.page-header-yue {
  width: 90%;
  height: 50%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  padding: 24rpx 0;
}

.divider-line {
  width: 90%;
  min-height: 2rpx;
  background-color: rgb(244, 244, 244);
}

.page-header-wx .img {
  width: 8%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.page-header-wx .img image {
  width: 40rpx;
  height: 40rpx;
}

.page-header-wx .label {
  width: 42%;
  height: 100%;
  font-size: 28rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.page-header-wx .balance {
  width: 42%;
  height: 100%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-right: 12rpx;
}

.page-header-wx .selectImg {
  width: 8%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.page-header-wx .selectImg image {
  width: 40rpx;
  height: 40rpx;
}

.page-header-yue .img {
  width: 8%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.page-header-yue .img image {
  width: 40rpx;
  height: 40rpx;
}

.page-header-yue .label {
  width: 42%;
  height: 100%;
  font-size: 28rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.page-header-yue .balance {
  width: 42%;
  height: 100%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-right: 12rpx;
}

.page-header-yue .selectImg {
  width: 8%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.page-header-yue .selectImg image {
  width: 40rpx;
  height: 40rpx;
}

.goods-content {
  width: 100%;
  height: 196rpx;
  display: flex;
  flex-direction: row;
  background-color: white;

}

.goods-image-content {
  width: 30%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 12rpx;
}

.goods-detail-content {
  width: 70%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.goods-detail-content .title {
  font-size: 32rpx;
  font-weight: 800rpx;
  color: rgb(14, 14, 14);
  margin-top: 12rpx;
  height: 50%;
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.goods-detail-content .price_tx {
  font-size: 32rpx;
  font-weight: 500rpx;
  color: rgb(14, 14, 14);
  height: 50%;
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-top: 4rpx;
}

.goods-detail-content .price {
  font-size: 28rpx;
  font-weight: 600;
  color: red;
  justify-content: center;
  align-items: center;
}

.goods-image-content image {
  width: 144rpx;
  height: 144rpx;
  border-radius: 16rpx;
  border: 1rpx solid rgb(221, 221, 221);
}

.goods-rule {
  width: 100%;
  min-height: 144rpx;
  margin-top: 16rpx;
  margin-bottom: 32rpx;
  background-color: white;
  display: flex;
  flex-direction: column;
  padding-left: 16rpx;
  padding-right: 16rpx;
  padding-top: 8rpx;
}

.goods-rule .goods-rule-title {
  width: 100%;
  min-height: 36rpx;
  font-size: 32rpx;
  color: rgb(14, 14, 14);
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-left: 32rpx;
  margin-right: 32rpx;
}

.goods-rule .goods-rule-content {
  width: 100%;
  min-height: 32rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: rgb(44, 44, 44);
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-left: 32rpx;
  margin-right: 32rpx;
}

.goods-rule-label {
  width: 100%;
  min-height: 42rpx;
  margin-top: 16rpx;
  margin-bottom: 32rpx;
  margin-left: 32prx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.goods-rule-label .img {
  width: 12%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.goods-rule-label .text {
  width: 88%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 24rpx;
  font-weight: 500;
  color: #939699;
}

.goods-rule-label .text text {
  font-size: 24rpx;
  font-weight: 500;
  color: #313233;
}

.goods-rule-label image {
  width: 40rpx;
  height: 40rpx;

}

.page-footer {
  width: 100%;
  height: 96rpx;
  bottom: 0;
  additive-symbols: row;
  position: fixed;
  display: flex;
  flex-direction: row;
  background-color: white;
}

.page-footer .left {
  padding-left: 20rpx;
}

.page-footer .left .price {
  color: #313233;
  font-size: 32rpx;
  font-weight: 500;
  width: 100%;
  padding-right: 24rpx;
  font-weight: 400;
  line-height: 44rpx;

}

.page-footer .left .number {
  color: #939699;
  font-size: 24rpx;
  font-weight: 500;
  width: 100%;
  line-height: 34rpx;
}

.page-footer .right {
  width: 70%;
  height: 100%;
  display: flex;
  color: white;
  background-color: #F6B352;
  justify-content: center;
  align-items: center;
}

.page-footer .right_disabled {
  width: 70%;
  height: 100%;
  display: flex;
  color: white;
  background-color: #939699;
  justify-content: center;
  align-items: center;
}

.col_wrap {
  display: flex;
  align-items: center;
  margin-left: 40rpx;
}

.clo_input {
  width: 80rpx;
  font-size: 24rpx;
  color: rgba(147, 150, 153, 1);
}

.go_rule {
  color: red !important;
}

.goods-buy-number {
  width: 90%;
  font-size: 28rpx;
  color: #939699;
  margin-top: 42rpx;
}

.goods-buy-delivery {
  width: 90%;
  color: #939699;
  font-size: 28rpx;
  margin-top: 42rpx;
  margin-bottom: 32rpx;
}

.add-delete-outer {
  /* display: inline-block; */
  float: right;

}

.add-delete-mod {
  display: inline-block;
  width: 68rpx;
  height: 60rpx;
  background-color: #F5F7FA;
  margin-right: 4rpx;
  text-align: center;
  font-size: 28rpx;
  line-height: 56rpx;
  color: #000;
}

.b-r-8 {
  border-top-right-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}

.b-l-8 {
  border-top-left-radius: 8rpx;
  border-bottom-left-radius: 8rpx;
}

.icon-box {
  float: right;
  margin-left: 60rpx;
  color: #000;
}

.freight-outer {
  width: 90%;
  padding: 24rpx 0;
}

.page-freight {
  display: flex;
  width: 100%;
  margin-top: 16rpx;
  background-color: #fff;
  font-size: 28rpx;
  color: #939699;
  justify-content: center;
}

.freight-charge {
  float: right;
  color: #000;
}

.my-arrows {
  display: inline-block;
  width: 17rpx;
  height: 17rpx;
  border-top: 1rpx solid #C4C8CC;
  border-right: 1rpx solid #C4C8CC;
  transform: rotate(45deg);
  margin-left: 20rpx;
}

.add-addr {
  float: right;
  color: #000;
}

.outer-address {
  width: 90%;
  padding: 24rpx 0;

}

.page-address {
  display: flex;
  width: 100%;
  margin-top: 16rpx;
  background-color: #fff;
  font-size: 28rpx;
  color: #939699;
  justify-content: center;
}

.page-detail {
  display: flex;
  width: 100%;
  margin-top: 16rpx;
  background-color: #fff;
  font-size: 28rpx;
  color: #939699;
  justify-content: center;
}

.detail-outer {
  width: 90%;
  padding: 24rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;

}

.detail-person {
  color: #000;
  font-weight: 600;
  margin-bottom: 10rpx;
}

.detail-person text {
  margin-right: 60rpx;
}

.update-icon {
  width: 60rpx;
  height: 60rpx;
}

.detail-address {
  width: 600rpx;
  word-break: normal;
}