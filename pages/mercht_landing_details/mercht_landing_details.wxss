
.container_page {
  width: 100%;
  height: 100%;
  flex-direction: column;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: white;  
}
.page-content{
  width: 100%;
  flex-direction: column;
  justify-content: center;
  align-items: center; 
  display: flex;
}
.page-skill-content{
  width: 100%;
  flex-direction: column;
  justify-content: center;
  align-items: center; 
  display: flex;
  margin: 16rpx;
}
.page-skill-content image{
  width: 196rpx;
  height: 196rpx;
}
.page-skill-content view{
  color: rgb(11, 11, 11);
  font-size: 28rpx;
}
.divider-line{
  width: 95%;
  height: 2rpx;
  background-color: aliceblue;
}
.page-collage-content{
  width: 100%;
  flex-direction: column;
  justify-content: center;
  align-items: center; 
  display: flex;
  margin: 16rpx;
}
.image-content {
  width: 256rpx;
  height: 256rpx;
  border-radius: 4rpx;
  border: 1px solid rgb(244, 244, 244);  
  flex-direction: column;
  justify-content: center;
  align-items: center; 
  display: flex;  
}
.page-collage-content image{
  width: 196rpx;
  height: 196rpx;
}
.page-collage-content view{
  color: rgb(11, 11, 11);
  font-size: 28rpx;
}