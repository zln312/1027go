.container_page {
  width: 100%;
  height: 100%;
}
.scroll-view-content {
  width: 100%;
  height: 100%;
  position: absolute;
  display: flex;
  background-color: whitesmoke;
}
.goods-content {
  width: 95%;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  display: flex;
  margin: 16rpx;
  border-radius: 24rpx;
  background-color: white;
}

.collage-dlg-goods-img {
  width: 30%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.collage-dlg-goods-img image {
  width: 90%;
  height: 144rpx;
  margin-top: 8rpx;
  margin-left: 8rpx;
  border: 1rpx solid #e3f2fd;
}

.collage-dlg-goods-text {
  width: 70%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;  
}

.collage-dlg-goods-text .goods-name {
  font-size: 28rpx;
  font-weight: bold;
  color: black;
  flex-wrap: nowrap;
}

.collage-dlg-goods-text .goods-prices {
  font-size: 28rpx;
  font-weight: normal;
  color: rgb(164, 164, 164);
  flex-wrap: nowrap;
}

.collage-dlg-goods-text .goods-quotas {
  font-size: 24rpx;
  font-weight: normal;
  color: rgb(164, 164, 164);
  flex-wrap: nowrap;
}
.divider-line-dashed {
  width: 90%;
  border: 1rpx dashed #e3f2fd;
   margin-top: 12rpx;
}
.goods-content .goods-prices {
  width: 20%;
  height: 100%;
