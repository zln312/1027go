  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.goods-content .goods-prices .goods-prices-l {
  font-size: 28rpx;
  color: red;
  font-weight: bold;
  text-align: center;
}

.goods-content .goods-prices .goods-prices-t {
