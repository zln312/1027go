<import src="../../components/appToast/appToast.wxml" />
<template is="toast" data="{{ ..._toast_ }}" />
<import src="/templates/cell.wxml" />
<view class="container_page">
     <view class="company-logo-content">
          <view class="company-logo-image">
            <image src='/images/contact_logo.png' mode='aspectFit' />      
          </view>
     </view>
     <view class="page-content">
          <view class="page-content-item">
              <view class="label">客服热线</view>
              <view class="number" bindtap="callCustom">{{hotService}}</view>      
          </view>
            <view class="divider-line"></view> 
          <view class="page-content-item">
               <view class="label">客服微信</view>
              <view class="number" bindtap="copyWx">{{customWx}}</view>            
          </view> 
          <view class="divider-line"></view>                
     </view>
</view> 